<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:gravity="center_horizontal"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:paddingLeft="10dp"
            android:paddingTop="10dp">

            <Button
                android:id="@+id/back_button"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:background="@mipmap/back_arrow"

                android:fontFamily="monospace"
                android:textAllCaps="false"
                android:textSize="15dp"
                android:textStyle="normal" />

            <Button
                android:id="@+id/buttonquestion"
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentTop="true"
                android:layout_marginEnd="10dp"
                android:layout_marginRight="10dp"
                android:background="@mipmap/help"
                android:fontFamily="monospace"
                android:gravity="center"
                android:paddingTop="15dp"
                android:textAllCaps="false"
                android:textSize="15dp" />

        </RelativeLayout>
    </LinearLayout>
    <ScrollView
        android:id="@+id/scroll"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:scrollbars="vertical">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginRight="20dp"
            android:layout_marginLeft="20dp"
            >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            >
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:paddingTop="10dp">

                <TextView
                    android:id="@+id/textView9"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_below="@+id/button3qe"
                    android:gravity="center"
                    android:textSize="23dp"
                    android:text="SELECT SYMPTOMS" />
            </LinearLayout>

            <TextView
                android:id="@+id/physical"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="10dp"
                android:gravity="center"
                android:text="PHYSICAL"
                android:textSize="17dp" />


            <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
                android:paddingTop="5dp"
                android:weightSum="3"
        android:gravity="center"
        >

        <CheckBox
            android:id="@+id/button1"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:background="@drawable/test_click"
            android:button="@null"
            android:gravity="center"
            android:text="Headaches"
            android:layout_weight="1"
            android:textColor="@drawable/test_text_click"
            android:textSize="15dp" />

        <CheckBox
            android:id="@+id/button2"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:layout_alignBaseline="@+id/button1"
            android:layout_alignBottom="@+id/button1"
            android:layout_centerHorizontal="true"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:background="@drawable/test_click"
            android:button="@null"
            android:gravity="center"
            android:layout_weight="1"
            android:text="Muscle Aches"
            android:textColor="@drawable/test_text_click"
            android:textSize="15dp" />


        <CheckBox
            android:id="@+id/button7"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:background="@drawable/test_click"
            android:button="@null"
            android:gravity="center"
            android:layout_weight="1"
            android:text="Trouble Sleeping"
            android:textColor="@drawable/test_text_click"
            android:textSize="15dp" />


    </LinearLayout>


            <TextView
                android:id="@+id/emotional"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="15dp"
                android:textSize="17dp"
                android:gravity="center"
                android:text="EMOTIONAL" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="5dp"
        android:gravity="center"
        android:weightSum="3"
        >
        <CheckBox
            android:id="@+id/button4"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:background="@drawable/test_click"
            android:button="@null"
            android:gravity="center"
            android:text="Sadness"
            android:layout_weight="1"
            android:textColor="@drawable/test_text_click"
            android:textSize="15dp" />

        <CheckBox
            android:id="@+id/button5"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:background="@drawable/test_click"
            android:button="@null"
            android:gravity="center"
            android:text="Irritability"
            android:layout_weight="1"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:textColor="@drawable/test_text_click"
            android:textSize="15dp"
            android:layout_alignBaseline="@+id/button1"
            android:layout_alignBottom="@+id/button1"
            android:layout_centerHorizontal="true" />

        <CheckBox
            android:id="@+id/button6"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:background="@drawable/test_click"
            android:button="@null"
            android:layout_weight="1"
            android:gravity="center"
            android:text="Restlessness"
            android:textColor="@drawable/test_text_click"
            android:textSize="15dp"
            android:layout_alignBaseline="@+id/button1"
            android:layout_alignBottom="@+id/button1"
            android:layout_toLeftOf="@+id/button3qe"
            android:layout_toStartOf="@+id/button3qe" />
    </LinearLayout>

            <TextView
                android:id="@+id/cognitive"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="15dp"
                android:textSize="17dp"
                android:gravity="center"
                android:text="COGNITIVE" />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="5dp"
        android:gravity="center"
        android:layout_weight="1"
        >
        <CheckBox
            android:id="@+id/button11"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:background="@drawable/test_click"
            android:button="@null"
            android:gravity="center"
            android:layout_weight="1"
            android:text="Impaired Concentration"
            android:textColor="@drawable/test_text_click"
            android:textSize="15dp"
            android:layout_alignBaseline="@+id/button1"
            android:layout_alignBottom="@+id/button1"
            android:layout_centerHorizontal="true" />

        <CheckBox
            android:id="@+id/button3"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:layout_weight="1"
            android:layout_alignBaseline="@+id/button1"
            android:layout_alignBottom="@+id/button1"
            android:layout_toLeftOf="@+id/button3qe"
            android:layout_toStartOf="@+id/button3qe"
            android:background="@drawable/test_click"
            android:button="@null"
            android:gravity="center"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:text="Language Barriers"
            android:textColor="@drawable/test_text_click"
            android:textSize="15dp" />

        <CheckBox
            android:id="@+id/button8"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:layout_weight="1"
            android:background="@drawable/test_click"
            android:button="@null"
            android:gravity="center"
            android:text="Anxious Feeling"
            android:textColor="@drawable/test_text_click"
            android:textSize="15dp"
            android:layout_alignBaseline="@+id/button1"
            android:layout_alignBottom="@+id/button1"
            android:layout_centerHorizontal="true" />


    </LinearLayout>

            <TextView
                android:id="@+id/behaviours"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="15dp"
                android:textSize="17dp"
                android:gravity="center"
                android:text="BEHAVIOURS" />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="5dp"
        android:gravity="center"
        android:weightSum="3"
        android:orientation="horizontal"
        >

        <CheckBox
            android:id="@+id/button9"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:background="@drawable/test_click"
            android:button="@null"
            android:gravity="center"
            android:layout_weight="1"
            android:text="Nail Biting"
            android:textColor="@drawable/test_text_click"
            android:textSize="15dp"
            android:layout_alignBaseline="@+id/button1"
            android:layout_alignBottom="@+id/button1"
            android:layout_toLeftOf="@+id/button3qe"
            android:layout_toStartOf="@+id/button3qe" />

        <CheckBox
            android:id="@+id/button10"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:background="@drawable/test_click"
            android:button="@null"
            android:gravity="center"
            android:layout_weight="1"
            android:layout_marginRight="5dp"
            android:layout_marginLeft="5dp"
            android:text="Social isolation"
            android:textColor="@drawable/test_text_click"
            android:textSize="15dp" />

        <CheckBox
            android:id="@+id/button12"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:background="@drawable/test_click"
            android:button="@null"
            android:gravity="center"
            android:text="Irregular eating"
            android:layout_weight="1"
            android:textColor="@drawable/test_text_click"
            android:textSize="15dp"
            android:layout_alignBaseline="@+id/button1"
            android:layout_alignBottom="@+id/button1"
            android:layout_toLeftOf="@+id/button3qe"
            android:layout_toStartOf="@+id/button3qe" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:layout_marginTop="20dp"
        android:gravity="center"
        >
    <Button
        android:id="@+id/done"
        android:layout_width="180dp"
        android:layout_height="50dp"
        android:layout_gravity="center"
        android:background="@drawable/done_click"
        android:paddingBottom="5dp"
        android:text="Done"
        android:textColor="@drawable/done_text_click" />
    </LinearLayout>
        </LinearLayout>
        </RelativeLayout>
    </ScrollView>
</LinearLayout>